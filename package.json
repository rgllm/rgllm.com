    "url": "git+https://github.com/rgllm/rgllm.com.git"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next dev",
    "prebuild": "yarn clean && echo All clean ✨",
    "build": "next build",
    "start:prod": "next start",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx .",
    "postbuild": "NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs && node ./scripts/generate-sitemap.mjs",
    "clean": "rimraf ./node_modules/.cache ./server/dist ./build ./public/build \"./app/styles/**/*.css\"",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md)\"",
    "test:unit": "jest --testPathIgnorePatterns=pages/api/tests && jest --projects pages/api/tests/jest.config.js",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@prisma/client": "4.4.0",
    "axios": "1.1.3",
    "@vercel/og": "0.0.19",
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "himalaya": "1.1.0",
    "html-attribute-to-react": "0.0.5",
    "html-metadata-parser": "2.0.4",
    "next": "12.3.1",
    "next-auth": "4.14.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.6.0",
    "react-to-print": "2.14.7",
    "recharts": "2.1.15",
    "slugify": "1.6.5",
    "swr": "1.3.0",
    "use-delayed-render": "0.0.7"
  },
  "devDependencies": {
    "@playwright/test": "1.27.1",
    "@tailwindcss/typography": "0.5.7",
    "@testing-library/react-hooks": "8.0.1",
    "@types/jest": "29.2.0",
    "@types/node": "17.0.23",
    "@types/react": "18.0.21",
    "autoprefixer": "10.4.12",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "globby": "13.1.2",
    "jest": "29.2.1",
    "jest-environment-jsdom": "29.2.1",
    "node-mocks-http": "1.11.0",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "prisma": "4.4.0",
    "rss": "1.2.2",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.4"
  }
