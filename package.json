{
	"name": "rgllm.com",
	"version": "6.0.0",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy -- --keep-vars",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"votes:dev": "bun --cwd packages/votes-api run dev",
		"votes:deploy": "bun --cwd packages/votes-api run deploy",
		"votes:test": "bun --cwd packages/votes-api run test"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.6.1",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-slot": "^1.2.3",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"lucide-react": "^0.536.0",
		"next": "15.3.5",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-markdown": "^10.1.0",
		"rehype-sanitize": "^6.0.0",
		"remark-gfm": "^4.0.1",
		"swr": "^2.3.4",
		"tailwind-merge": "^3.3.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@tailwindcss/typography": "^0.5.16",
		"@types/node": "^24.1.0",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"autoprefixer": "^10.4.21",
		"eslint": "^9",
		"eslint-config-next": "15.3.5",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"tailwindcss": "^4",
		"tw-animate-css": "^1.3.6",
		"typescript": "^5",
		"wrangler": "^4.26.0"
	}
}
