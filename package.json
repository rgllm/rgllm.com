    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "globby": "13.1.2",
    "jest": "28.1.3",
    "node-mocks-http": "1.11.0",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
