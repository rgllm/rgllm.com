    "date-fns": "2.29.1",
    "himalaya": "1.1.0",
    "html-attribute-to-react": "0.0.5",
    "html-metadata-parser": "2.0.4",
    "next": "12.2.3",
    "next-auth": "4.10.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.4.0",
    "react-to-print": "2.14.7",
